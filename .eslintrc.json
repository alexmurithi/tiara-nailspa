{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:solid/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["prettier"],
  "rules": {
    "solid/no-react-specific-props": "off"
  }
}
