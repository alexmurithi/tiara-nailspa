{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@containers/*": ["src/containers/*"],
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/layouts/*"],
      "@themes/*": ["src/themes/*"],
      "@styles/*": ["src/assets/jss/*"],
      "@images/*": ["src/assets/images/*"],
      "@utils/*": ["src/utils/*"]
    }
  },

  "exclude": ["node_modules"]
}
